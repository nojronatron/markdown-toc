    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.29.0",
    "eslint": "^8.41.0",
    "glob": "^10.4.2",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "latest": "^0.2.0"
