{
  "name": "jr-markdowntoc-vscode",
  "displayName": "Create Markdown TOC",
  "description": "Adds a table of contents to your markdown file.",
  "icon": "images/jrsoftware-logo-128x128.png",
  "galleryBanner.color": "#424BB2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nojronatron/markdown-toc.git"
  },
  "version": "0.3.1",
  "author": "Jon Rumsey",
  "pricing": "Free",
  "engines": {
    "vscode": "^1.90.0"
  },
  "publisher": "jon-rumsey-dev",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
